{
  "manifest_version": 2,
  "name": "Extension Example",
  "description": "Sample Chrome Extension",
  "version": "1.0.0",
  "background": {
    "scripts": ["extension-main.js"]
  },
  "browser_action": {
    "default_popup": "extension-window.html"
  },
  "icons": {
    "38": "images/buddy-38.png",
    "128": "images/buddy-128.png"
  },
  "permissions": [
    "tabs",
    "https://localhost/",
    "http://.sourceallies.com/"
  ],
  "externally_connectable": {
    "ids": ["mfnpjdefoieghfkdilkaninlealillfa"],
    "matches": [
      "http://localhost/*",
      "*://sourceallies.com/*"
    ]
  },
  "content_scripts": [{
    "matches": ["*://www.sourceallies.com/*"],
    "js": ["content-script-catify.js"]
  }],
  "web_accessible_resources": [
    "images/*.*"
  ]
}